<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
<script src="https://kit.fontawesome.com/39f20aadc9.js" crossorigin="anonymous"></script>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
<link href='https://unpkg.com/boxicons@2.0.7/css/boxicons.min.css' rel='stylesheet'>


<script type="text/javascript">window.searchClearButtonLabelLocalized = "{{t 'search_clear'}}";</script>
<script src="https://kit.fontawesome.com/39f20aadc9.js" crossorigin="anonymous"></script>

<footer class="footer">
  <div class="footer-inner">

  {{#is settings.minimal_footer false}}
<div class="footer-links">  
  		<div class="footer-logo">
        {{#link 'help_center'}}
        <img src="{{settings.logo_footer}}">
        {{/link}}
        </div>
      <div>
        {{#is settings.footer_title_1 true}}
          <a href="{{settings.footer_link_1}}">{{settings.footer_title_text_1}}</a>
        {{/is}}
      </div>
      <div>
        {{#is settings.footer_title_2 true}}
         <a href="{{settings.footer_link_2}}">{{settings.footer_title_text_2}}</a>
        {{/is}}
      </div>
      <div>
        {{#is settings.footer_title_3 true}}
           <a href="{{settings.footer_link_3}}">{{settings.footer_title_text_3}}</a>
        {{/is}}
      </div>
      <div>
        {{#is settings.footer_title_4 true}}
           <a href="{{settings.footer_link_4}}">{{settings.footer_title_text_4}}</a>
        {{/is}}
      </div>      
	</div>
  {{/is}}
    
<!--     Socials -->
     <div class="container socials">
       {{#isnt settings.facebook_url "#"}}
      <button class="btn1">
        <a href="{{settings.facebook_url}}">
    <img src="{{asset 'Social_Icon_01.png'}}">
        </a>
  </button>
  {{/isnt}}
       
  {{#isnt settings.instagram_url "#"}}
      <button class="btn1">
        <a href="{{settings.instagram_url}}">
    <img src="{{asset 'Social_Icon_02.png'}}">
        </a>
  </button>
  {{/isnt}}
         
  {{#isnt settings.linkedin_url "#"}}      
  <button class="btn1">
        <a href="{{settings.linkedin_url}}">
    <img src="{{asset 'Social_Icon_03.png'}}">
        </a>
  </button>
  {{/isnt}}
  
    {{#isnt settings.twitter_url "#"}}
  <button class="btn1">
        <a href="{{settings.twitter_url}}">
    <img src="{{asset 'Social_Icon_04.png'}}">
        </a>
  </button>
    {{/isnt}}
    
  </div>
  </div>
  <div class="footer-brand">
    <p>{{'Designed & Developed by'}}<a href="https://www.dynanet.support/" target="_blank" >{{' DynaNet Online Support'}}</a>
    </p>
  </div>
  
</footer>
